{
	"name": "itphilosophy-bpmsoft-git-changed-schemas",
	"displayName": "itphilosophy-bpmsoft-git-changed-schemas",
	"description": "Отображение изменённых схем BPMSoft в текущей ветке Git.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.99.0"
	},
	"categories": [
		"SCM Providers",
		"Other"
	],
	"main": "./dist/extension.js",
	"l10n": "./l10n",
	"contributes": {
		"views": {
			"scm": [
				{
					"id": "bpmsoftGitChangedSchemas",
					"name": "%extension.bpmsoftGitChangedSchemas%",
					"icon": "media/changed-schemas.svg"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@types/vscode": "^1.99.1",
		"@typescript-eslint/eslint-plugin": "^8.28.0",
		"@typescript-eslint/parser": "^8.28.0",
		"@vscode/l10n-dev": "^0.0.18",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^9.23.0",
		"ts-loader": "^9.5.2",
		"typescript": "^5.8.2",
		"webpack": "^5.98.0",
		"webpack-cli": "^6.0.1"
	},
	"dependencies": {
		"@vscode/l10n": "^0.0.10"
	},
	"extensionDependencies": [
		"vscode.git"
	]
}
